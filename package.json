{
  "name": "monoux",
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "syncpack": "^13.0.0",
    "turbo": "^2.1.3"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.11.0",
  "private": true,
  "scripts": {
    "add:app": "pnpm --filter @monoux/gen add:app",
    "add:package": "pnpm --filter @monoux/gen add:package",
    "build": "turbo build",
    "clean": "turbo clean",
    "dev": "turbo dev",
    "format": "biome --write \"**/*.{ts,tsx}\"",
    "lint": "turbo lint",
    "test": "turbo test",
    "vert:dev": "turbo vert:dev"
  }
}
