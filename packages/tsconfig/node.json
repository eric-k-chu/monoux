{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node JS",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "ESNext",
    "allowJs": false,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "noEmit": false,
    "emitDeclarationOnly": true,
    "resolveJsonModule": true,
    "target": "ESNext"
  }
}
